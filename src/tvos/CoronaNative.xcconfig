// This file contains basic setup for Corona Native apps and plugins

CORONA_ROOT=$HOME/Library/Application Support/Corona/Native

FRAMEWORK_SEARCH_PATHS = $(inherited) "$CORONA_ROOT/Corona/tvos/frameworks/"

LIBRARY_SEARCH_PATHS = $(inherited) "${PROJECT_DIR}/../shared/Flurry"

HEADER_SEARCH_PATHS = $(inherited) "$CORONA_ROOT/Corona/tvos/frameworks/CoronaCards.xcframework/tvos-arm64/CoronaCards.framework/PrivateHeaders" "${PROJECT_DIR}/../shared/Flurry"

BITCODE_GENERATION_MODE = bitcode
